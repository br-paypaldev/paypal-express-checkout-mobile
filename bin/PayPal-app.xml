<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/2.6">
	<id>com.paypal.x.mobile</id>
	<filename>com.paypal.x.mobile</filename>
	<name>PayPal Code Sample</name>

	<versionNumber>1.0.0</versionNumber>

	<!-- <description></description> -->
	<copyright>(c) 2011</copyright>

	<initialWindow>
		<content>com.paypal.x.mobile.swf</content>
		<title>PayPal Mobile</title>
		<autoOrients>true</autoOrients>
	</initialWindow>

	<installFolder>paypal</installFolder>

	<programMenuFolder>PayPal</programMenuFolder>

	<icon>
		<image16x16>img/app/paypal-icon_16x16.png</image16x16>
		<image32x32>img/app/paypal-icon_32x32.png</image32x32> 
		<image48x48>img/app/paypal-icon_48x48.png</image48x48>
		<image72x72>img/app/paypal-icon_72x72.png</image72x72>
		<image114x114>img/app/paypal-icon_114x114.png</image114x114>
		<image128x128>img/app/paypal-icon_128x128.png</image128x128>
	</icon>

	<android>
		<manifestAdditions>
		<![CDATA[
		<manifest android:installLocation="auto">
			<uses-permission android:name="android.permission.INTERNET"/>
			<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
			<supports-screens android:normalScreens="false"/>
		</manifest>
		]]>
		</manifestAdditions>
	</android>
</application>